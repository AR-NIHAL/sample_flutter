//sum of digits
#include <iostream>
using namespace std;

int sumOfDigits(int n) {
    int sum = 0;
    while (n!=0) {
        int last = n% 10;
        sum = sum+last;
        //sum+=last;
        n = n/10;
        //n /= 10;
    }
    return sum;
}

int main() {
    int n= 12345;
    cout <<sumOfDigits(n);
    return 0;
}
